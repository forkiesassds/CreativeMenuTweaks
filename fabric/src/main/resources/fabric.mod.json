{
  "schemaVersion": 1,
  "id": "tweaked_creative_menu",
  "version": "${version}",
  "name": "Tweaked Creative Menu",
  "description": "A mod that improves the creative menu, including animations, sounds, and data driven item groups!",
  "authors": [
    "icanttellyou",
    "GoopSwagger"
  ],
  "contact": {
    "homepage": "https://modrinth.com/project/creative-menu-tweaks",
    "sources": "https://github.com/Lucifixion/CreativeMenuTweaks"
  },
  "license": "MIT",
  "icon": "assets/tweaked_creative_menu/icon.png",
  "environment": "*",
  "accessWidener": "tweaked_creative_menu.accesswidener",
  "entrypoints": {
    "main": [
      "me.icanttellyou.tweakedcreativemenu.fabric.TweakedCreativeMenuFabric"
    ],
    "client": [
      "me.icanttellyou.tweakedcreativemenu.fabric.client.TweakedCreativeMenuFabricClient"
    ],
    "modmenu": [
      "me.icanttellyou.tweakedcreativemenu.fabric.client.ModMenuIntegration"
    ]
  },
  "mixins": [
    "tweaked_creative_menu-common.mixins.json",
    "tweaked_creative_menu-fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.15.7",
    "minecraft": ">=1.21 <=1.21.1",
    "java": ">=21"
  },
  "recommends": {
    "yet_another_config_lib_v3": "*"
  }
}